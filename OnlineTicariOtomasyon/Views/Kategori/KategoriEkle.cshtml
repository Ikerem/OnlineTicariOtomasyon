@model OnlineTicariOtomasyon.Models.Siniflar.Kategori
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script src="~/Scripts/jquery-3.4.1.js"></script>
<br/>
<h2>Yeni Kategori Ekle</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.KategoriAd)
   @Html.TextBoxFor(x=>x.KategoriAd,new {@clas="form-control"})
    <br/>
    <button class="btn btn-info" id="btn1" >Kaydet</button>
    <script>
        $("#btn1").click(function () {
            alert("Kategori Eklendi!")
        });

        
    </script>
</form>

